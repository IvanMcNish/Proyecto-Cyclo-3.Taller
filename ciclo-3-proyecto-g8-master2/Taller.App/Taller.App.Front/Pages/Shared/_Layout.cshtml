<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Taller.App.Front</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Taller.App.Front.styles.css" asp-append-version="true" />
</head>

<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm border-bottom box-shadow mb-3 navbar-dark bg-primary">
            <div class="container">
                <a class="navbar-brand" asp-area="" asp-page="/Index">Taller Inlekema</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse"
                    aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                            <li class="nav-item">

                                <a class="nav-link text-light" asp-area="" asp-page="/OrdenesPendientes/OrdenesPendientes">Ordenes Pendientes</a>
                            </li>
                            <li class="nav-item">

                                <a class="nav-link text-light" asp-area="" asp-page="">Gestion Inventario</a>
                            </li>
                            <ul class="navbar-nav">
                                <li class="nav-item dropdown">
                                <a class="nav-link dropdown-toggle" href="#" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                    Herramientas
                                </a>
                                <ul class="dropdown-menu dropdown-menu-dark">
                                    <li><a class="dropdown-item" asp-page="/GestionSupervisor/RegistrarNuevoVehiculo/RegistrarNuevoVehiculo">Registrar Vehiculo</a></li>
                                    <li><a class="dropdown-item" asp-page="/GestorVehiculos/GestorVehiculos">Gestor de Vehiculos</a></li>
                                    <li><a class="dropdown-item" asp-page="/GestionSupervisor/ConsultaInfoVehiculo/ConsultaInfoVehiculo">Consultar info Vehiculo</a></li>
                                    <li><a class="dropdown-item" asp-page="/GestionSupervisor/ConsultaInfoVehiculoReg/ConsultaInfoVehiculoReg">Consultar vehiculos registrados</a></li>
                                    <li><a class="dropdown-item" asp-page="/GestionSupervisor/RegistrarNuevoMecanico/RegistrarNuevoMecanico">Registrar Mecanico</a></li>
                                    <li><a class="dropdown-item" asp-page="/GestionSupervisor/RegistrarInfoNuevoUsuario/RegistrarInfoNuevoUsuario">Registrar Nuevo Usuario (Supervisor)</a></li>
                                </ul>
                                </li>
                            </ul>
                            <li class="nav-item">

                                <a class="nav-link text-light" asp-area="" asp-page="/Resgiclievehi/Resgiclievehi">Gestor Cliente</a>
                            </li>
                   
                            <li class="nav-item">
                                <a class="nav-link text-light" asp-area="" asp-page="/RegistroCliente/RegistroCliente">Registro Cliente</a>
                            </li>



                             @*
                            <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/Privacy">Privacy</a>
                            </li>
                            <li class="nav-item">

                            <a class="nav-link text-dark" asp-area="" asp-page="/Mecanico/GestionMecanico">Mecánicos</a>
                            </li>
                            <li class="nav-item">
                            <a class="nav-link text-dark" asp-area=""
                            asp-page="/Propietario/RegistroPropietario">Propietario</a>
                            </li> *@





                            <li class="nav-item">
                                <a class="nav-link text-light" asp-area="" asp-page="/Solicservicio/Solicservicio">Solicitar servio</a>
                            </li>

                            <li class="nav-item">
                                <a class="nav-link text-light" asp-area="" asp-page="/Ordenparatecnico/Ordenparatecnico">Trabajos pendientes</a>
                            </li>










                    </ul>
                </div>
            </div>
        </nav>
    </header>

    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2022 - Taller.App.Front - <a asp-area="" asp-page="/Privacy">Privacy</a>
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>

</html>